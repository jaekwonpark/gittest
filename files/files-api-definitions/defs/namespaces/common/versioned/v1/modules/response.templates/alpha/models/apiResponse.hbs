
components:
  schemas:
    {{packageName}}.{{responseModelType}}:
      type: "object"
      properties:
        data:
      {{#if listType}}
          type: "array"
          items:
            $ref: "ModelRef({{=<% %>=}} {<%responseModelRef%>}<%={{ }}=%>)"
      {{else}}
          $ref: "ModelRef({{=<% %>=}} {<%responseModelRef%>}<%={{ }}=%>)"
      {{/if}}
        metadata:
          $ref: "ModelRef({/namespaces/common/versioned/v1/modules/response.types/alpha/models/ApiResponseMetadata})"
          description: "REST Response for {{#if listType}} list of {{/if}} {{responseModelSimpleName}} object{{#if listType}}s{{/if}}"
